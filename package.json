{
  "name": "gulp-cache",
  "version": "0.2.4",
  "description": "A cache proxy task for Gulp",
  "scripts": {
    "test": "jshint **/*.js && mocha",
    "coverage": "jshint **/*.js && mocha --require blanket -R html-cov > coverage.html && open coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jgable/gulp-cache.git"
  },
  "keywords": [
    "gulpplugin"
  ],
  "author": "Jacob Gable <jacob.gable@gmail.com> (http://jacobgable.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgable/gulp-cache/issues"
  },
  "dependencies": {
    "bluebird": "^2.2.2",
    "cache-swap": "^0.1.1",
    "gulp-util": "^3.0.0",
    "lodash-node": "^3.2.0",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "should": "^5.0.0",
    "mocha": "^2.1.0",
    "jshint": "^2.6.0",
    "sinon": "^1.12.2"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "/lib/TaskProxy.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "contributors": [
    {
      "name": "Jacob Gable",
      "email": "jacob.gable@gmail.com",
      "url": "https://github.com/jgable",
      "contributions": 46,
      "hireable": false
    },
    {
      "name": "Tyler Akins",
      "email": "fidian@rumkin.com",
      "url": "https://github.com/fidian",
      "contributions": 4,
      "hireable": false
    }
  ]
}
