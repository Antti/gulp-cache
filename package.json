{
  "name": "gulp-cache",
  "version": "0.2.2",
  "description": "A cache proxy task for Gulp",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/jshint/bin/jshint **/*.js && ./node_modules/mocha/bin/mocha -R spec",
    "coverage": "./node_modules/jshint/bin/jshint **/*.js && ./node_modules/mocha/bin/mocha --require blanket -R html-cov > coverage.html && open coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jgable/gulp-cache.git"
  },
  "keywords": [
    "gulpplugin"
  ],
  "author": "Jacob Gable <jacob.gable@gmail.com> (http://jacobgable.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgable/gulp-cache/issues"
  },
  "devDependencies": {
    "should": "~2.1.1",
    "mocha": "~1.17.0",
    "jshint": "~2.4.1",
    "sinon": "~1.7.3"
  },
  "dependencies": {
    "bluebird": "^2.2.2",
    "cache-swap": "0.0.6",
    "gulp-util": "^3.0.0",
    "lodash-node": "~2.4.1",
    "map-stream": "~0.1.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "/lib/TaskProxy.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "contributors": [
    {
      "name": "Jacob Gable",
      "email": "jacob.gable@gmail.com",
      "url": "https://github.com/jgable",
      "contributions": 46,
      "hireable": false
    },
    {
      "name": "Tyler Akins",
      "email": "fidian@rumkin.com",
      "url": "https://github.com/fidian",
      "contributions": 4,
      "hireable": false
    }
  ]
}
